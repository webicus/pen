<script src="/javascripts/application.js" type="text/javascript"></script> 
        <script src="/javascripts/jquery.js" type="text/javascript"></script> 
        <script src="/javascripts/jquery.dimensions.js" type="text/javascript"></script> 
        <script src="/javascripts/jquery.accordion.js" type="text/javascript"></script> 
        <script type="text/javascript"> 
            jQuery.noConflict();
            jQuery(document).ready(function(){
                jQuery.ui.accordion.animations.superbounce = function(options){
                    this.slide(options, {
                        easing: options.down ? "swing" : "swing",
                        duration: 1250
                    });
                    
                };
                
                
                jQuery(".basic").accordion({
                    animated: 'superbounce',autoheight: false,alwaysOpen: false, header:'.head' 
                });
            });
			


        </script> 